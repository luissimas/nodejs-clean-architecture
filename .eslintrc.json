{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "prettier", "standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-useless-constructor": "off",
    "@typescript-eslint/no-useless-constructor": ["error"]
  }
}
